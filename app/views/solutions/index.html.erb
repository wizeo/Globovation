<h1>Listing solutions</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Description</th>
      <th>Votes</th>
      <th>Admin approval</th>
      <th>Project ID</th>
      <th>User ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solutions.each do |solution| %>
      <tr>
        <td><%= solution.id %></td>
        <td><%= solution.title %></td>
        <td><%= solution.description %></td>
        <td><%= solution.votes %></td>
        <td><%= solution.admin_approval %></td>
        <td><%= solution.project_id %></td>
        <td><%= solution.user_id%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

